<?php
class Clase1{
    
//atributo
private $atributo1;
private $atributo2;
private $atributo3;

//metodo constructor
public function __construct($atributo1, $atributo2, $atributo3){
    $this -> atributo1 = $atributo1;
    $this -> atributo2 = $atributo2;
    $this -> atributo3 = $atributo3;
}
//metodos de acceso get
public function getAtributo1() { return $this -> atributo1; }
public function getAtributo2() { return $this -> atributo2; }
public function getAtributo3() { return $this -> atributo3; }

//metodos de acceso set
public function setAtributo1($atributo1) { $this -> atributo1 = $atributo1; }
public function setAtributo2($atributo2) { $this -> atributo2 = $atributo2; }
public function setAtributo3($atributo3) { $this -> atributo3 = $atributo3; }

//metodo toString
public function __toString(){
    return "Atributo1: " . $this -> atributo1 . "\n" .
           "Atributo2: " . $this -> atributo2 . "\n" .
           "Atributo3: " . $this -> atributo3 . "\n";
}
}